{
  "name": "fidio",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --watchAll -u --coverage",
    "test:pre-commit": "CI=true react-scripts test -u --coverage",
    "eject": "react-scripts eject",
    "lint:fix": "eslint src/**/*.js --fix",
    "pretty": "./node_modules/.bin/prettier --single-quote --write \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "contributors:add": "npx all-contributors add",
    "contributors:generate": "npx all-contributors generate"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && yarn test:pre-commit"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "node-sass": "4.12.0",
    "react": "16.9.0",
    "react-dom": "16.9.0",
    "react-router-dom": "^5.0.1",
    "react-scripts": "3.1.1",
    "redux": "4.0.4",
    "redux-thunk": "2.3.0"
  },
  "devDependencies": {
    "@babel/cli": "7.4.4",
    "@babel/core": "7.4.5",
    "@babel/plugin-proposal-class-properties": "7.3.4",
    "@babel/plugin-transform-runtime": "7.4.4",
    "@babel/preset-env": "7.4.5",
    "all-contributors-cli": "6.1.2",
    "electron": "5.0.6",
    "husky": "3.0.4",
    "lint-staged": "9.2.3",
    "prettier": "1.18.2",
    "react-redux": "7.1.0",
    "rimraf": "2.6.3"
  }
}
