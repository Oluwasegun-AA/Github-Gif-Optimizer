{
  "name": "fidio",
  "version": "0.1.0",
  "private": true,
  "main": "dist/app/index.js",
  "homepage": "./",
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-scripts build",
    "app": "electron .",
    "build:electron": "babel src/ -d dist --copy-files",
    "electron:dev": "yarn clear && yarn build:electron && concurrently \" BROWSER=none yarn start\" \"wait-on http://localhost:3000 && yarn app\"",
    "pretty": "./node_modules/.bin/prettier --single-quote --write \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "lint": "eslint src/**/*.js --fix",
    "test:ci": "CI=true react-scripts test --colors --coverage",
    "contributors:init": "all-contributors init",
    "contributors:add": "npx all-contributors add",
    "contributors:generate": "npx all-contributors generate",
    "test": "react-scripts test --watchAll -u --coverage",
    "eject": "react-scripts eject",
    "clear": "rimraf dist build"
  },
  "husky": {
    "hooks": {
      "pre-push": "lint-staged && yarn test:ci"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@babel/preset-react": "7.0.0",
    "@babel/runtime": "7.4.5",
    "electron": "6.0.4",
    "enzyme": "3.10.0",
    "enzyme-adapter-react-16": "1.14.0",
    "fluent-ffmpeg": "2.1.2",
    "lodash": "4.17.15",
    "node-sass": "4.12.0",
    "prop-types": "15.7.2",
    "react": "16.13.1",
    "react-dom": "16.9.0",
    "react-redux": "7.1.0",
    "react-router-dom": "^5.0.1",
    "react-scripts": "3.4.1",
    "redux": "4.0.4",
    "redux-thunk": "2.3.0"
  },
  "devDependencies": {
    "@babel/cli": "7.4.4",
    "@babel/core": "7.4.5",
    "@babel/plugin-proposal-class-properties": "7.3.4",
    "@babel/plugin-transform-runtime": "7.4.4",
    "@babel/preset-env": "7.4.5",
    "@babel/register": "7.5.5",
    "all-contributors-cli": "6.1.2",
    "concurrently": "4.1.2",
    "electron-builder": "21.2.0",
    "enzyme-to-json": "3.4.0",
    "eslint": "^6.2.2",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "husky": "3.0.4",
    "lint-staged": "9.2.3",
    "prettier": "1.18.2",
    "react-app-rewired": "2.1.3",
    "rimraf": "3.0.0",
    "wait-on": "3.3.0"
  }
}
